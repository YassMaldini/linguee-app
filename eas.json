{
  "cli": {
    "version": ">= 2.2.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "IS_DEV": "true",
        "APP_NAME": "Linguee (dev)",
        "IOS_BUNDLE_ID": "com.ios.dev.lingueeClone",
        "ANDROID_PACKAGE_NAME": "com.android.dev.lingueeClone"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "env": {
        "APP_NAME": "Linguee clone (NOT OFFICIAL)",
        "IOS_BUNDLE_ID": "com.ios.lingueeclone",
        "ANDROID_PACKAGE_NAME": "com.android.lingueeclone"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
